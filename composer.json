{
    "name": "katanox/katanox-php",
    "type": "library",
    "description": "The official PHP SDK of the Katanox APIs",
    "keywords": [
        "katanox",
        "api",
        "hotels",
        "hotel-api",
        "travel-sellers",
        "rest",
        "api",
        "marketplace"
    ],
    "homepage": "https://github.com/katanox/katanox-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Katanox Engineering Team",
            "email": "developers@katanox.com",
            "homepage": "https://katanox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "doctrine/collections": "^1.6.1",
        "guzzlehttp/guzzle": "^7.3",
        "netresearch/jsonmapper": "^4.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Katanox\\": "src/Katanox"
        },
				"files": ["src/Katanox/Model/ModelHelper.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Katanox\\": "tests/Katanox"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix-style": "vendor/bin/php-cs-fixer fix --rules=@PSR12,@PhpCsFixer . -vvv --cache-file .php_cs.cache"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
