{
    "name": "katanox/katanox-php",
    "type": "library",
    "description": "The official PHP SDK of the Katanox APIs",
    "keywords": [
        "katanox",
        "api",
        "hotels",
        "hotel-api",
        "travel-sellers",
        "rest",
        "api",
        "marketplace"
    ],
    "homepage": "https://github.com/katanox/katanox-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Katanox Engineering Team",
            "email": "developers@katanox.com",
            "homepage": "https://katanox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "Katanox\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Katanox\\Test\\" : "test/" }
    }
}
